{
  "name": "back2front",
  "version": "2.0.0",
  "description": "Modular isomorphic web framework",
  "keywords": [
    "back2front",
    "isomorphic",
    "modular"
  ],
  "homepage": "https://github.com/heeroluo/back2front#readme",
  "bugs": {
    "url": "https://github.com/heeroluo/back2front/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Heero.Luo",
    "email": "heeroluo@126.com",
    "url": "http://heeroluo.net/"
  },
  "scripts": {
    "dev": "node ./bin/www --env dev --no-prod",
    "test": "node ./bin/www --env test --no-prod",
    "pre": "node ./bin/www --env pre --no-prod",
    "prod": "node ./bin/www --env prod --no-prod",
    "build-dev": "node ./node_modules/back2front-cli/bin/index build . --env dev",
    "build-test": "node ./node_modules/back2front-cli/bin/index build . --env test",
    "build-pre": "node ./node_modules/back2front-cli/bin/index build . --env pre",
    "build-prod": "node ./node_modules/back2front-cli/bin/index build . --env prod",
    "depa": "node ./node_modules/back2front-cli/bin/index depa ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heeroluo/back2front.git"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "cookie-parser": "^1.4.4",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "minimist": "^1.2.0",
    "morgan": "^1.9.1",
    "require-dir": "^1.2.0",
    "serve-favicon": "~2.5.0",
    "xtemplate": "~4.6.0"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "back2front-cli": "^2.3.4",
    "postcss": "^7.0.36",
    "postcss-load-config": "^2.1.0",
    "postcss-px2rem": "^0.3.0",
    "precss": "^4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 20 versions",
    "not ie <= 8"
  ]
}
